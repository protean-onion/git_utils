SHA 268a723ccf535a9534b19d10e81ff7056cac6425:
#!/usr/bin/python3
# coding: utf-8

from subprocess import run, Popen
from os import listdir
import sys

def list_sha():
    sha_list = []
    objects = ".git/objects"
    for directory in listdir(objects):
        if len(str(directory)) <= 2:
            sha_list.append(directory + listdir(objects + "/" + directory)[0])
    return sha_list
    
def retreive_content(sha):
    object_content = run(f"git cat-file -p {sha}", shell=True, capture_output=True).stdout.decode("unicode_escape")
    return object_content

def dict_content():
    sha_list = list_sha()
    content_dict = {}
    for sha in sha_list:
        content_dict[sha] = retreive_content(sha)
    return content_dict

def generate_contents():
    line = "-------------------------
"
    content_dict = dict_content()
    contents = ""
    for sha in content_dict.keys():
        ornate_content = line + "SHA: " + sha + "
" + content_dict[sha] + "

"
        contents += ornate_content
    return contents

def main():
    contents = str(generate_contents())
    # print(contents)
#    return contents

if __name__ == "__main__":
    sys.exit(main())

