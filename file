SHA cebf66043ac0048ee791250830e61a0d175b1d04:
#!/usr/bin/python3
# coding: utf-8

from subprocess import run
from os import listdir
import sys

def list_sha():
    sha_list = []
    objects = ".git/objects"
    for directory in listdir(objects):
        if len(str(directory)) <= 2:
            sha_list.append(directory + listdir(objects + "/" + directory)[0])
    return sha_list
    
def retreive_content(sha):
    object_content = run(f"git cat-file -p {sha}", shell=True, capture_output=True).stdout.decode("unicode_escape")
    return object_content

def list_content():
    content_list = []
    for sha in list_sha():
        content_list.append(retreive_content(sha))
    return content_list

def main():
    sha_list = list_sha()
    line = "
--------------------

"
    
    for sha in sha_list:
        return_content = "SHA " + sha + ":
" + retreive_content(sha) + line

if __name__ == "__main__":
    sys.exit(main())

